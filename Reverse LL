class Solution {
    public ListNode reverseList(ListNode head) {
        ListNode curr=head;
        ListNode prev=null;
        while(curr!=null){
            if(head==null){
                return head;
            }
            ListNode curr1=curr.next;
            curr.next=prev;
            prev=curr;
            curr=curr1;
        }
        head=prev;
        return head;
    }
